[user]
    email = justinjl@umich.edu
	name = Justin Jaesung Lee
[core]
	editor = vim
	autocrlf = false
[push]
	default = simple
[credential]
	helper = store
[color]
	ui = auto
[pull]
	ff = only
[diff]
	tool = vimdiff
[difftool]
	prompt = true
[merge]
	tool = vimdiff
[init]
	defaultBranch = master
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
