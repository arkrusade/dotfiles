#!/bin/bash
set -e
cd ~/home
for file in $(comm <(git ls-tree master --name-only) <(sort <(cat nocopy.txt)) -3 -2)
do
    # hard or soft link?
    # ln -f ~/home/$file ~/$file
    if [ -O ~/$file ]
    then 
        echo linking ~/home/$file ~/$file
        ln -sf ~/home/$file ~/$file
    else
        echo ~/$file already exists
    fi
done

